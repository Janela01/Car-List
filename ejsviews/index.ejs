<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs')%> 
<body>
      <%- include('./partials/nav.ejs')%> 
      <div class="cars content">
            
            <% if(cars.length>0) { %>
                  <h2>All Cars : </h2>
                  <% cars.forEach(car =>{ %>
                        <a class="" href="/cars/<%=car._id%>">
                              <h3 class="model"><%=car.model%></h3>
                              <p class="imp"><%=car.imp%></p>
                        </a>
                  <% }) %> 
            <% console.log(cars.length)} else {%>
                  <h2>There Are No Cars Avalible In Database</h2>
            <% } %>  
      </div>
      <%- include('./partials/footer.ejs')%> 
</body>
</html>